<?xml version="1.0"?>
<package >
	<metadata>

		<id>Elephant7</id>
		<title>Elephant7</title>

		<version>2.0.1</version>
		<releaseNotes>Dotnet standard release.</releaseNotes>

		<authors>Joel Viney</authors>
		<owners>JoelViney</owners>

		<!-- <iconUrl></iconUrl> -->
		<projectUrl>https://github.com/JoelViney/Elephant7</projectUrl>
		<license type="expression">MIT</license>
		<copyright>Copyright @2018 Joel Viney</copyright>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>

		<summary>Provides extension methods for System.Random for generating more complex data.</summary>
		<description>
			Provides extension methods for System.Random for generating more complex data.
			This includes basic methods like NextBoolean, NextCurrency, NextDateTime, NextDecimal etc.
			As well as more advanced options like NextEnum, NextListItem, NextParameter.
			Finally it also contains random  objects like NextPerson and NextAddress (Australian Format).
		</description>

		<!-- Tags appear in the gallery and can be used for tag searches -->
		<tags>Random Test</tags>

		<dependencies>
			<group targetFramework="netstandard2.0" />
		</dependencies>

		<contentFiles>
			<files include="Elephant7\Data\*.*" buildAction="EmbeddedResource" />
		</contentFiles>
	</metadata>
	<files>
		<file src="Elephant7\bin\Debug\netstandard2.0\Elephant7.dll" target="lib\netstandard2.0" />
		<file src="Elephant7\bin\Debug\netstandard2.0\Elephant7.xml" target="lib\netstandard2.0" />
	</files>
</package>